<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #F4F1DE;" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="TitleScreen.TitleScreenController">
   <children>
